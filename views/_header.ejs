<link rel="stylesheet" href="/css/header.css">  
<header>
   <a href="/">JassaIn logo</a> 
  <ul>
        <% if (locals.user){ %>
            <li>
                <a href="/">
                    <%= user.name %>
                </a>
            </li>
            <li>
                <a href="/users/sign-out">Sign Out</a>
            </li>
        <% }else{ %>

            <li>
                <a href="/users/sign-in">Sign In</a>
            </li>

            <li>
                <a href="/users/sign-up">Sign Up</a>
            </li>


        <% } %>
    </ul>
  

</header>